#include <iostream>
#include <string>

using namespace std;

class Combine {
private:
    string str;

public:
    Combine(string s = "") {
        str = s;
    }

    Combine operator+(Combine obj) {
        Combine temp;
        temp.str = str + obj.str;
        return temp;
    }

    void display() {
        cout << str << endl;
    }
};

int main() {
    Combine s1("xyz"), s2("ppr");
    Combine s3;

    s3 = s1 + s2;

    cout << "Concatenated string: ";
    s3.display();

    return 0;
}
