#include <iostream>
#include <string>

using namespace std;

class Person {
protected:
    string name;
    int age;

public:
    void accept() {
        cout << "Enter Name: ";
        cin >> name;
        cout << "Enter Age: ";
        cin >> age;
    }

    void display() {
        cout << "Name: " << name << endl;
        cout << "Age: " << age << endl;
    }
};

class Student : protected Person {
private:
    int rollno;

public:
    void accept() {
        Person::accept();
        cout << "Enter Roll Number: ";
        cin >> rollno;
    }

    void display() {
        Person::display();
        cout << "Roll Number: " << rollno << endl;
    }
};

int main() {
    Student s1;

    s1.accept();
    s1.display();

    return 0;
}
