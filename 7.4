#include <iostream>

using namespace std;

class Student {
private:
    int count;

public:
    Student(int c = 0) {
        count = c;
    }

    void operator++() {
        ++count;
    }

    void display() {
        cout << "Student count: " << count << endl;
    }
};

int main() {
    Student s1(10);

    ++s1;

    cout << "After negation: " << endl;
    s1.display();

    return 0;
}
