#include <iostream>

using namespace std;

template <class T>
T arrsum(T arr[], int n) {
    T sum = 0;
    for (int i = 0; i < n; i++) {
        sum = sum + arr[i];
    }
    return sum;
}

int main() {
    int arr[] = {5, 1, 2, 3, 4, 63};
    int n = sizeof(arr) / sizeof(arr[0]);

    int sum = arrsum(arr, n);

    cout << "Sum of array: " << sum << endl;

    return 0;
}
